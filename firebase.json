{
  "hosting": [
    {
      "target": "website",
      "public": "website/build/yew",
      "cleanUrls": true,
      "redirects": [
        // LEGACY (since 2020-08-15)
        {
          "source": "/docs/v/zh_cn",
          "destination": "/docs/zh-CN/intro/",
          "type": 301
        },
        {
          "source": "/docs/v/zh_tw",
          "destination": "/docs/zh-TW/intro/",
          "type": 301
        },
        {
          "regex": "/docs/v/zh_cn/(.*)",
          "destination": "/docs/zh-CN/:1",
          "type": 301
        },
        {
          "regex": "/docs/v/zh_tw/(.*)",
          "destination": "/docs/zh-TW/:1",
          "type": 301
        }
      ]
    },
    {
      "target": "examples",
      "public": "dist",
      "rewrites": [
        {
          "source": "/router/**",
          "destination": "/router/index.html"
        },
        {
          "source": "/todomvc/**",
          "destination": "/todomvc/index.html"
        }
      ]
    }
  ]
}