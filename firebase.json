{
  "hosting": [
    {
      "target": "website",
      "public": "website/build/",
      "cleanUrls": true,
      "redirects": []
    },
    {
      "target": "api",
      "public": "api-docs/dist/",
      "cleanUrls": true,
      "redirects": [
        {
          "source": "/next",
          "destination": "/next/yew",
          "type": 302
        },
        {
          "source": "/",
          "destination": "https://docs.rs/yew",
          "type": 302
        }
      ]
    },
    {
      "target": "examples",
      "public": "dist",
      "rewrites": [
        {
          "source": "/router/**",
          "destination": "/router/index.html"
        },
        {
          "source": "/todomvc/**",
          "destination": "/todomvc/index.html"
        }
      ],
      "redirects": [
        {
          "source": "/",
          "destination": "https://yew.rs/getting-started/examples",
          "type": 302
        }
      ]
    }
  ],
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  }
}
